{
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "rules": {
    "quotes": 0,
    "strict": 0,
    "no-unused-vars": 0,
    "no-shadow": 0,
    "new-cap": 0,
    "no-undef": 0,
    "no-underscore-dangle": 0,
    "no-console": "off"
  },
  "env": {
    "node": true,
    "browser": true
  }
}
